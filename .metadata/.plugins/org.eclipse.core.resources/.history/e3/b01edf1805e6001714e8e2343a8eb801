/**
 *
 */

/**
 * @author internousdev
 *
 */
public class Person {

	/**
	 * @param args
	 */
	public String name = null;
	public int age = 0;

	public Person(String name,int age){
		this.name=name;
		this.age=age;
	}

	public String getName(){
		return this.name;
	}

	public void setName(){
		this.name=name;
	}
}
